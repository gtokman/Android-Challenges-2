<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.garytokman.tokmangary_ce08">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activities.FormActivity">
            <intent-filter>
                <action android:name="com.fullsail.android.jav2ce08.ACTION_ADD_PERSON"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".Activities.DetailActivity">
            <intent-filter>
                <action android:name="com.fullsail.android.jav2ce08.ACTION_VIEW_PERSON"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>

        </activity>
    </application>

</manifest>